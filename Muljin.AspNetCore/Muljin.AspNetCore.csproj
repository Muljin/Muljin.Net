<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Muljin.AspNetCore</PackageId>
    <Version>1.1.0</Version>
    <Authors>Mahmoud Swehli</Authors>
    <Company>Muljin Pte. Ltd</Company>
    <Description>Muljin common classes and utilities for Asp.Net Core</Description>
    <RepositoryUrl>https://github.com/muljin/muljin.net</RepositoryUrl>
    <PackageProjectUrl>https://www.muljin.com</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Product>Muljin.AspNetCore</Product>
    <RootNamespace>Muljin.AspNetCore</RootNamespace>
    <AssemblyName>Muljin.AspNetCore</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Extensions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Muljin.Common\Muljin.Common.csproj" />
  </ItemGroup>
</Project>
